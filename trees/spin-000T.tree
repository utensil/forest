\import{macros}

\tag{spin}

\refdef{Spin group}{dereli2010degenerate}{
The group #{\operatorname{Spin}(p, q)} is defined by
##{
\operatorname{Spin}(p, q)=\left\{v_1 \ldots v_m \in \mathcal{C} \ell_{p, q} \mid m \in 2 \mathbb{Z}^{+}, v_i=\sum_{j=1}^{p+q} a_{i j} e_j,\left\langle v_i, v_i\right\rangle=\mp 1,1 \leq i \leq m\right\}
}
}