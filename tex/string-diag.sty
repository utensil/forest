\newcommand{\strlabr}[2]{
 \node at ($(#1) + (0.5,0)$) {#2}}

\newcommand{\strlabl}[2]{
 \node at ($(#1) + (-0.5,0)$) {#2}}

\newcommand{\strlabur}[2]{
